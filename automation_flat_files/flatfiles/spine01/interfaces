# Loopback interface
auto lo
iface lo inet loopback
  address 10.0.0.21/32
  alias loopback interface

auto eth0
iface eth0
  vrf mgmt
  address 10.255.6.7/23
  gateway 10.255.6.1

auto mgmt
iface mgmt
    address 127.0.0.1/8
    vrf-table auto

auto swp1
iface swp1

auto swp2
iface swp2

auto swp13
iface swp13

auto swp14
iface swp14

auto swp15
iface swp15

auto swp16
iface swp16
