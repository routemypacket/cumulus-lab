# Loopback interface
auto lo
iface lo inet loopback
  address 10.0.0.21/32
  alias loopback interface

auto eth0
iface eth0 inet dhcp
    vrf mgmt

auto mgmt
iface mgmt
    address 127.0.0.1/8
    vrf-table auto

auto swp1
iface swp1
  alias Connects to Leaf01

auto swp2
iface swp2
  alias Connects to Leaf02

auto swp13
iface swp13
  alias Connects to Exit01

auto swp14
iface swp14
  alias Connects to Exit02

auto swp15
iface swp15
  alias Connects to Spine01

auto swp16
iface swp16
  alias Connects to Spine02
